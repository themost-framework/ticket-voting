{
  "name": "express-server-project",
  "version": "0.1.0",
  "description": "Express Application with MOST Web Framework",
  "main": "dist/www.js",
  "repository": "",
  "private": true,
  "author": "",
  "scripts": {
    "start": "node bin/www.js",
    "clean": "rimraf dist/server",
    "test": "jasmine",
    "build": "themost build",
    "themost": "themost",
    "serve": "nodemon bin/www.js",
    "debug": "nodemon --inspect-brk bin/www.js"
  },
  "devDependencies": {
    "@themost-devkit/build-typescript": "^0.1.4",
    "@themost/cli": "^2.5.0",
    "@types/angular": "^1.6.57",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.17.0",
    "@types/jasmine": "^3.5.12",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.0",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/superagent": "^4.1.9",
    "@types/supertest": "^2.0.10",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.6.1",
    "jasmine": "^3.6.1",
    "jasmine-spec-reporter": "^5.0.2",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@themost/client": "^2.5.1",
    "@themost/common": "^2.5.2",
    "@themost/data": "^2.5.21",
    "@themost/express": "^1.5.13",
    "@themost/mailer": "^2.1.5",
    "@themost/node": "^2.5.0",
    "@themost/query": "^2.5.0",
    "@themost/sqlite": "^2.2.3",
    "@themost/web": "^2.5.2",
    "@themost/xml": "^2.5.1",
    "bootstrap": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "ejs-locals": "^1.0.2",
    "express": "~4.16.0",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.7"
  },
  "nodemonConfig": {
    "watch": [
      "server/",
      "./bin/www.js"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --require ts-node/register"
    },
    "env": {
      "NODE_ENV": "development",
      "TS_NODE_FILES": "true",
      "PORT": "3030"
    },
    "ext": "ts,js,json"
  }
}
